{
  "compilerOptions": {
    "target": "ES6",
    "module": "Node16",
    "moduleResolution": "node16",
    "outDir": "./dist",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "#mock/*": ["src/mock/*/index.ts"],
      "#member/*": ["src/member/*/index.ts"],
      "#controller/*": ["src/core/controller/index.ts"],
      "#adapter/*": ["src/core/adapter/*.ts"],
      "#repository/*": ["src/repository/*.ts"],
      "#config/*": ["src/core/config/*.ts"],
      "#util/*": ["src/core/util/*.ts"],
      "#error/*": ["src/core/error/*.ts"]
    }
  },
  "include": [
    "src/**/*.ts",
    "index.ts",
    "src/socialLogin/dto/frontend/response/MockScore"
  ],
  "exclude": ["node_modules"]
}
